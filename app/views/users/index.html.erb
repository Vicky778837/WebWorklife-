<h1 class='text-center'>Dashboard</h1>
<div class="container  mt-2">
   <div class="justify-content-center">
      <% @users.each do |art|%>
         <div class="card mb-3 m-auto m-0" style="width: 18rem;">
            <div class="card-body">
               <h5 class="card-title">Name: <%=art.name%></h5>
               <p class="card-title">Email <%=art.email%></p>
               <p class="card-title">Total Number of Article: <%=art.articles.count%></p>
               <p class="card-title"><%=link_to"Show Article","#{}",class: "btn btn-outline-primary"%></p>

            </div>
         </div>
      <% end %>
</div>


